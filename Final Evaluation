#First Two
def first_two(str):
  if len(str) < 2:
    return str
  else:
    return str[0:2]

#Combo String
def combo_string(a, b):
  if len(a) > len(b):
    return b + a + b
  else:
    return a + b + a



#Count Code
def count_code(str):
  
  ctr = 0
  for i in range(0,len(str) - 3,1):
    if str[i:i+2] == "co" and str[i+3] == "e":
      ctr += 1
  
  return ctr
  
