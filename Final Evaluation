#First Two
def first_two(str):
  if len(str) < 2:
    return str
  else:
    return str[0:2]



#Count Code
def count_code(str):
  
  ctr = 0
  for i in range(0,len(str) - 3,1):
    if str[i:i+2] == "co" and str[i+3] == "e":
      ctr += 1
  
  return ctr

#Lone Sum
def lone_sum(a, b, c):
 sum = 0
 
 if a != b and a != c: sum += a
 if b != a and b != c: sum += b
 if c != a and c != b: sum += c
 
 return sum


#Count Hi
 def count_hi(str):
 
  count = 0
  for i in range(0, len(str)- 1, 1):
    if str[i:i+2] == "hi":
      count += 1
      
  return count
  
 
 
 #Cat Dog
 def cat_dog(str):
  
  count_cat = 0
  count_dog = 0
  
  for i in range(len(str) - 2):
    if str[i:i+3] == "dog":
      count_dog += 1
    elif str[i:i+3] == "cat":
      count_cat += 1
      
  if count_cat == count_dog:
    return True
  else:
    return False
